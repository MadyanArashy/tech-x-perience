{
  "expo": {
    "name": "tech-x-perience",
    "slug": "tech-x-perience",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/techXperience-logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/techXperience-long.png",
      "resizeMode": "cover",
      "backgroundColor": "#151515"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/techXperience-logo.png",
        "backgroundColor": "#151515"
      },
      "package": "com.madyanarashy.techxperience"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/techXperience-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#151515"
        }
      ],
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ebe8e785-cb3b-46a2-98a4-79beb1f7a0d3"
      }
    }
  }
}
